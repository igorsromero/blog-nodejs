<%- include("../../partials/header.ejs") %>
<%- include("../../partials/navbar.ejs") %>
    <body>
        <div class="container">
            <hr />
            <h2>Usuários</h2>
            <a class="btn btn-success" href="/admin/users/create">Criar novo usuário</a>
            <hr />
            <table class="table table-bordered">
              <thead>
                <tr>
                  <th>Id</th>
                  <th>E-mail</th>
                </tr>
              </thead>
              <tbody>
                <% users.forEach((user => { %>
                  <tr>
                    <td><%= user.id %></td>
                    <td><%= user.email %></td>
                  </tr>
                  <% })) %>
              </tbody>
            </table>
          </div>
    </body>
<%- include("../../partials/footer.ejs") %>